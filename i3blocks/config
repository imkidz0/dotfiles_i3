markup=pango

[music]
command=~/.config/i3blocks/scripts/mpd_status.sh
interval=1

[wifi]
command=~/.config/i3blocks/scripts/wifi_status.sh
interval=5

[cpu_temp]
command=~/.config/i3blocks/scripts/cpu_temp.sh
interval=5

[memory]
label=MEM:
command=$HOME/.config/i3blocks/scripts/memory_usage.sh
interval=5

[battery]
command=~/.config/i3blocks/scripts/battery_dual.sh
interval=5

[volume]
command=~/.config/i3blocks/scripts/volume.sh
interval=1
signal=29

[time]
command=date '+%a %I:%M:%S %p'
interval=1
